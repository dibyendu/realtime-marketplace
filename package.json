{
  "name": "real-time-marketplace-app",
  "version": "1.0.0",
  "description": "A Real-Time Marketplace application built using NodeJs(Express+Pug), React, MongoDB, socket.io and ArcGis APIs",
  "main": "server.js",
  "author": "Dibyendu Das",
  "license": "GPL-3.0",
  "dependencies": {
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.5",
    "express": "^4.15.3",
    "mongodb": "^2.2.27",
    "pug": "^2.0.0-rc.1",
    "socket.io": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "webpack": "^2.5.1"
  },
  "scripts": {
    "dev": "webpack -d --watch --config webpack.config.js",
    "prod": "webpack -p --config webpack.config.js",
    "build": "webpack --config webpack.config.js",
    "start": "webpack --config webpack.config.js && node server.js"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  }
}
